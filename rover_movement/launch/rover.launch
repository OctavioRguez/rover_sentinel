<?xml version="1.0"?>
<launch>
        <!-- Parameters file -->
        <rosparam file="$(find rover_sentinel)/config/puzzlebot.yaml"/>
        <rosparam file="$(find rover_sentinel)/config/classification.yaml"/>

        <include file="$(find rplidar_ros)/launch/rplidar_a1.launch" />

        <!-- Nodes -->
        <node name="navigation_obstacle_avoidance" pkg="rover_sentinel" type="navigation.py" output="screen"/>
        <node name="rover_sentinel_pov" pkg="rover_sentinel" type="cameraImage.py"/>
        <node name="rover_sentinel_prediction" pkg="rover_sentinel" type="detection.py"/>
</launch>